<script>



	$(function() 
	{

		$("#chkall").click(function(){
			$('input:checkbox').not(this).prop('checked', this.checked);
		});
		
		$('.txtcalendar').datepicker({
				format: 'yyyy-mm-dd',
				autoclose: true,
				changeMonth: true,
				changeYear: true,
				
		});

		function transsuccess() 
		{
			toastr.success('Process Complete');
		};
		
		function transwarning() 
		{
			toastr.warning('');
		};
		
		function transerror() 
		{
			toastr.options.fadeOut = 5000;
			toastr.error('');
			

		};
		
		
		
		
			
	});
	
	 $(document).on("click", ".buttonclick", function (event) 
	 {
			
		var temp= confirm("Are you sure?\n\n Click OK Button to proceed. Otherwise, Click Cancel Button");
		if (!temp)
		{
			return false;
		}
		$('#processingmodal').modal('show');
	});

</script>
	<div id="processingmodal" class="modal fade">
		<div class="modal-dialog modal-m">
			<div class="modal-content">
				<div class="modal-header"><h3 style="margin:0;">Processing</h3></div>
				<div class="modal-body">
					<div class="progress progress-striped active" style="margin-bottom:0;"><div class="progress-bar" style="width: 100%"></div></div>
				</div>
			</div>
		</div>
	</div>

